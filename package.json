    "@nestjs/core": "^11.0.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^6.3.1",
    "@ucast/mongo2js": "^1.3.4",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "bull": "^4.16.5",
