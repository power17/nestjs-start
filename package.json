    "joi": "^17.13.3",
    "mongoose": "^8.10.0",
    "mysql2": "^3.12.0",
    "nest-winston": "^1.10.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "preview-email": "^3.1.0",
    "prisma-mysql": "link:prisma/clients/mysql",
    "prisma-postgresql": "link:prisma/clients/postgresql",
